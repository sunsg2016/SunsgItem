<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#ffffffff"
    android:id="@+id/root"
    tools:context="com.breadtrip.BaseJavaTestActivity" >

    <Button
        android:id="@+id/Btn_fanxing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="泛型" />
    <Button
        android:id="@+id/Btn_data_format"
        android:layout_toRightOf="@+id/Btn_fanxing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="simpleDataFormat"/>
    
    <Button 
        android:id="@+id/btn_comparable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="comparable"
        android:layout_toRightOf="@+id/Btn_data_format"
        />
    
    <Button 
        android:id="@+id/btn_observal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="observable"
        android:layout_below="@+id/Btn_data_format"
        />
    
     <Button 
        android:id="@+id/btn_pattern"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="pattern"
        android:layout_below="@+id/Btn_data_format"
        android:layout_toRightOf="@+id/btn_observal"
        />
     
     <Button 
        android:id="@+id/btn_hashmap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hashmap"
        android:layout_below="@+id/btn_pattern"
        />
     
     <Button 
        android:id="@+id/btn_threadPoolExecutor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="threadPoolExecutor"
        android:layout_toRightOf="@+id/btn_hashmap"
        android:layout_below="@+id/btn_pattern"
        />
     
     <Button 
        android:id="@+id/btn_future_task"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="futureTask"
        android:layout_toRightOf="@+id/btn_threadPoolExecutor"
        android:layout_below="@+id/btn_pattern"
        />
     
     <Button 
        android:id="@+id/btn_view_attache_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="viewAttachTyhpe"
        android:layout_below="@+id/btn_hashmap"
        />
     <com.sunsg.item.view.CounterView 
         android:layout_width="200dp"
         android:layout_height="200dp"
         android:layout_toRightOf="@+id/btn_view_attache_type"
         android:layout_below="@+id/btn_hashmap"
         />
     
     

</RelativeLayout>
